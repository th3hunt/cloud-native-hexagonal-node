{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src", "./config"],
        "alias": {
          "logger": "./src/logger"
        }
      }
    ]
  ]
}
